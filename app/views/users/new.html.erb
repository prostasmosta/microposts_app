<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @user, data: { turbo: false } do |f| %>
      <%= render 'form', f: f %>
      <div class="my-3"><%= f.submit "Create my account", class: "btn btn-primary mb-3" %></div>
    <% end %>
  </div>
  <div><%= link_to 'back', :back %></div>
</div>
